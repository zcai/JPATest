<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">	
	<persistence-unit name="JPA_test_persistence">
      <provider>org.hibernate.ejb.HibernatePersistence</provider>
      <class>edu.harvard.channing.regepi.hibernate.test.Authorcontribution</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Comment</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Reviewer</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Reviewinfo</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Manuscriptchecklist</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Dataidform</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Status</class>
      <class>edu.harvard.channing.regepi.hibernate.test.Attachment</class>
      <shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
	</persistence-unit>

</persistence>